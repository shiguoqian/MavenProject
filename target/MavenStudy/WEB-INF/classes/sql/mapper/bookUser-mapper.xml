<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sgq.dao.IBookUserDao">
    <select id="getBookUserByName" parameterType="string" resultType="bookuser">
        select * from bookuser where name = #{name}
    </select>
</mapper>